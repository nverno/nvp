#include <bits/stdc++.h>
#include "../../../include/template.hpp"
using namespace std;

void solve() {
    $0
}

int main(int argc, char *argv[]) {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int t = 1;
    read(t);
    for (int i = 0; i < t; ++i) {
        write("Case #", i+1, ": ");
        solve();
    }

    return EXIT_SUCCESS;
}
