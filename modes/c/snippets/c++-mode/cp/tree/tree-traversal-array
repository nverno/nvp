# -*- mode: snippet -*-
# name: tree traversal array
# key: t_trav
# --
int timer = 0;
array<int,2> subtree[${1:maxN}];

void traversal(int u, int p, vector<int>& vals) {
    subtree[u][0] = timer++;
    for (auto& v : g[u]) {
        if (v == p) continue;
        traversal(v, u, vals);
    }
    subtree[u][1] = timer-1;
}
