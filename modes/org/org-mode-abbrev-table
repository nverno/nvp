(define-abbrev-table 'org-mode-abbrev-table
  '(
    ("noi" "\\noindent" nil :system t)
    ("bigo" ["\\mathcal{O}()" 1 nil nil] expand-abbrev-hook :system t)
    ("amssymb" "#+LATEX_HEADER: \\usepackage{amssymb}" nil :system t)
    )
  "Org-Mode Abbrevs."
  :parents (list prog-mode-abbrev-table))

(define-abbrev-table 'org-agenda-mode-abbrev-table
  '()
  "Org-Agenda-Mode Abbrevs."
  :parents (list prog-mode-abbrev-table))
