(define-abbrev-table 'wat-mode-abbrev-table
  '()
  "Wat-Mode Abbrevs."
  :enable-function #'nvp-abbrev-expand-p)

(define-abbrev-table 'wast-mode-abbrev-table
  '()
  "Wast-Mode Abbrevs."
  :parents (list wat-mode-abbrev-table)
  :enable-function #'nvp-abbrev-expand-p)
